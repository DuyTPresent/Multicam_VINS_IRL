<launch>

    <arg name="vins_path" default="$(find multicam_vins)/config/" />
    <arg name="config_path" default="$(find multicam_vins)/config/basler_camera/basler_camera.yaml"/>
    
    <node name="Multicam_VINS_node" pkg="multicam_vins" type="multicam_estimator" output="screen" >
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$arg vins_path" />
    </node>

    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log" args="-d $(find multicam_vins)/config/mcvio_4camera.rviz" />

</launch>
